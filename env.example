# Configuration KuCoin API
# Obtenez vos clés sur https://www.kucoin.com/account/api
KUCOIN_API_KEY=your_api_key_here
KUCOIN_API_SECRET=your_api_secret_here  
KUCOIN_PASSPHRASE=your_passphrase_here

# Mode sandbox (true pour tests, false pour trading réel)
KUCOIN_SANDBOX=true

# Configuration optionnelle du logging
LOG_LEVEL=INFO

# Configuration avancée (optionnel)
# SCAN_INTERVAL=60
# VOLUME_THRESHOLD=50
# DEFAULT_POSITION_SIZE=100